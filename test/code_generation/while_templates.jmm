class WhileTemplates {
    public static void main(String[] args) {
        int x;

        x = 0;

        // Can be optimized
        while (x < 5) {
            io.println(x);
            x = x + 1;
        }

        // Can be optimized
        while (false) {
            io.println(12345);
        }

        // Cannot be optimized (?)
        while (MathUtils.random(0, 4) < 2 && x < 9) {
            io.println(x);
            x = x + 1;
        }
    }
}